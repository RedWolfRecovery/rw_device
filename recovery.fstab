# mount point   fstype  device                                  device2                         flags
/boot           emmc    /dev/block/bootdevice/by-name/boot                                      flags=display="Boot";ose;dumwolfresize
/cache          ext4    /dev/block/bootdevice/by-name/cache                                     flags=display="Cache"
/recovery       emmc    /dev/block/bootdevice/by-name/recovery                                  flags=display="Recovery";backup=1;dumwolfresize
/system         ext4    /dev/block/bootdevice/by-name/system                                    flags=display="System";ose
/system_image   emmc    /dev/block/bootdevice/by-name/system                                    flags=display="System Image";backup=1;flashimg=1;ose
/data           ext4    /dev/block/bootdevice/by-name/userdata
/cust           ext4    /dev/block/bootdevice/by-name/cust                                      flags=display="Cust";wipeingui
/misc           emmc    /dev/block/bootdevice/by-name/misc                                      flags=display="Misc"
/efs1           emmc    /dev/block/bootdevice/by-name/modemst1                                  flags=display="EFS";backup=1
/splash         emmc    /dev/block/bootdevice/by-name/splash                                    flags=display="Boot Logo";backup=1;flashimg=1
/modem          emmc    /dev/block/bootdevice/by-name/modem                                     flags=display="Modem";backup=1;flashimg=1
/firmware       emmc    /dev/block/bootdevice/by-name/sec                                       flags=display="Firmware";backup=1
/dsp            emmc    /dev/block/bootdevice/by-name/dsp                                       flags=subpartitionof=/modem;backup=1
/efs2           emmc    /dev/block/bootdevice/by-name/modemst2                                  flags=subpartitionof=/efs1;backup=1
/efsc           emmc    /dev/block/bootdevice/by-name/fsc                                       flags=subpartitionof=/efs1;backup=1
/efsg           emmc    /dev/block/bootdevice/by-name/fsg                                       flags=subpartitionof=/efs1;backup=1
/vendor         ext4    /dev/block/bootdevice/by-name/cust                                      flags=display="Vendor";wipeingui;ose;backup=1
/vendor_image   emmc    /dev/block/bootdevice/by-name/cust                                      flags=backup=1;flashimg=1;ose

# Bootloader
/aboot          emmc    /dev/block/bootdevice/by-name/aboot                                     flags=backup=1;display="Bootloader"
/cmnlib         emmc    /dev/block/bootdevice/by-name/cmnlib                                    flags=backup=1;subpartitionof=/aboot
/hyp            emmc    /dev/block/bootdevice/by-name/hyp                                       flags=backup=1;subpartitionof=/aboot
/tz             emmc    /dev/block/bootdevice/by-name/tz                                        flags=backup=1;subpartitionof=/aboot
/rpm            emmc    /dev/block/bootdevice/by-name/rpm                                       flags=backup=1;subpartitionof=/aboot
/keymaster      emmc    /dev/block/bootdevice/by-name/keymaster                                 flags=backup=1;subpartitionof=/aboot
/devcfg         emmc    /dev/block/bootdevice/by-name/devcfg                                    flags=backup=1;subpartitionof=/aboot

# Removable storage
/usb_otg        auto    /dev/block/sdg1                         /dev/block/sdg                  flags=display="USB Storage";storage;wipeingui;removable
